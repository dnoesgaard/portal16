<div class="container--desktop m-t-1 ng-cloak" ng-if="datasetEvent.event.occurrenceCount > 0">
    <h4 class="card-header--bold" tx="map.nGeoreferencedRecords" tx-nr="datasetEvent.event.occurrenceCount"></h4>
    <div class="card">
        <div style="height: 400px">
            <adhoc-map no-drawing="true" filter="{dataset_key: datasetEvent.datasetKey, event_id: datasetEvent.eventKey}" map-style="{suggested: 'SQUARE_DOTS'}" class="fitSize fitSize--noBar"></adhoc-map>
        </div>
    </div>
</div>